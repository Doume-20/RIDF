sapin(n):={
  crayon vert;
  si n<=1 alors return; fsi;
  triangle_plein(10*n,10*n);
  tourne_gauche ;
  triangle_plein(10*n,10*n);
  saute 8*n;
  tourne_droite;
  sapin(4/5*n);
  pas_de_cote -8*n;
};

sap(n):={
  triangle_plein(5*n,5*n);
  tourne_gauche ;
  triangle_plein(5*n,5*n);
  saute 4*n;
  triangle_plein(4*n,4*n);
  tourne_droite ;
  triangle_plein(4*n,4*n);
  pas_de_cote -4*n;
};

sapins(n):={
  si n<=1 alors return; fsi;
  sap(n);
  saute 10*n;
  sapins(n-2);
};

