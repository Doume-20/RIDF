  si n<=1 alors crayon jaune; disque 10; crayon vert; return; fsi;
  crayon vert;
  sap(n);
  tourne_gauche 30;
  saute 10*n;
  tourne_droite 30;
  sapins2(n-2);
  tourne_droite 30;
  saute 10*n;
  tourne_gauche 30;
  sap(n);
};,
